<script type="text/javascript">
    $(function(){
      $("body"). on("click" , ".btnCopy", function(){
       var element = $(this).attr("data-copy");
       var $temp = $("<input>");
       $("body").append($temp);
       $temp.val(element).select();
       document.execCommand("copy");
       $temp.remove();
       console.log("Copied");
       $.notify({
            icon: "ti-info",
            message: "Link copied to clipboard"
          },{
            type: "warning",
            timer: 1000,
            delay: 700,
            placement: {
                from: 'top',
                align: 'center'
            }
          });
      });
    })

    $(document).ready(function () {
        $("body").on("click", "#sample", function () {
            var quesItm = $(this).attr("data-item");
            $.ajax({
            type: 'get',
            url: '/author_getCommittedSampleQue?id=' + quesItm,
            cache: false,
            success: function (returndata) {
                if (returndata) {
                // $('#bsModal3 .modal-body p').modal('show'); // Please right this in your Code
                // console.log(returndata);
                
                  $('#bsModal3 .modal-body p').html(returndata);
                  MathJax.Hub.Queue(["Typeset", MathJax.Hub]);
                  $('#bsModal3').modal('show');
                } else {
                // other code
                }
            },
                error: function () {
                console.error('Failed to process ajax !');
                }
            });
        });
      });
  </script>
